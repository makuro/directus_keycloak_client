{
  "name": "makuro/directus_keycloak_client",
  "description": "Keycloak OAuth 2.0 Client Provider for the headless CMS directus based on the PHP League OAuth2-Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Eric Delaporte",
      "email": "eric.delaporte@build-ideas.de",
      "homepage": "https://github.com/makuro"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "keycloak",
    "directus"
  ],
  "require": {
    "php": ">=7.2",
    "league/oauth2-client": "^2.0 <2.3.0",
    "firebase/php-jwt": "^6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.4.3",
    "mockery/mockery": "~1.2.4",
    "squizlabs/php_codesniffer": "~3.5.2"
  },
  "autoload": {
    "psr-4": {
      "Makuro\\Directus\\KeycloakClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Makuro\\Directus\\KeycloakClient\\Test\\": "test/src/"
    }
  }
}